{
  "name": "projectacme",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "node-start": "nodemon ./bin/www",
    "build-css": "node-sass --include-path scss scss/main.scss public/stylesheets/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "start": "parallelshell \"npm run node-start\" \"npm run watch-css\""
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "hbs": "*",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2",
    "request": "*"
  },
  "devDependencies": {
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "parallelshell": "^2.0.0"
  }
}
